{
  "name": "ember-cli-concat",
  "version": "1.0.1",
  "description": "An Ember addon that enables you to concatinate Ember CLI's app and vendor files into a single JS file and a single CSS file",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "node tests/runner",
    "autotest": "mocha --watch --reporter spec tests/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sir-dunxalot/ember-cli-concat.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "broccoli-file-remover": "^0.3.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-sourcemap-concat": "^0.4.1"
  },
  "devDependencies": {
    "broccoli": "^0.16.2",
    "broccoli-asset-rev": "^2.0.2",
    "chai": "^2.1.0",
    "chai-fs": "git://github.com/sir-dunxalot/chai-fs.git#master",
    "ember-cli": "0.2.3",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "0.0.8",
    "ember-cli-htmlbars": "0.7.4",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.16.1",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-export-application-global": "^1.0.2",
    "ember-try": "0.0.4",
    "glob": "^5.0.5",
    "lodash": "^3.8.0",
    "mocha": "^2.2.4",
    "mocha-jshint": "^2.2.3",
    "through": "^2.3.7",
    "tmp-sync": "^1.0.1"
  },
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "concatenate",
    "vendor",
    "performance",
    "scripts"
  ],
  "ember-addon": {
    "before": "broccoli-asset-rev",
    "configPath": "tests/dummy/config"
  }
}
