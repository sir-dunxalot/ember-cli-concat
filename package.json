{
  "name": "ember-cli-concat",
  "version": "0.2.1",
  "description": " An Ember addon that concatinates Ember CLI's app and vendor files into a single JS file and a single CSS file in production environments",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sir-dunxalot/ember-cli-concat.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "broccoli-file-remover": "^0.3.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-sourcemap-concat": "^0.4.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-ember-hbs-template-compiler": "^1.6.1",
    "ember-cli": "0.1.15",
    "ember-cli-6to5": "^3.0.0",
    "ember-cli-app-version": "0.3.1",
    "ember-cli-content-security-policy": "0.3.0",
    "ember-cli-dependency-checker": "0.0.7",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.7",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.14.1",
    "ember-export-application-global": "^1.0.2",
    "express": "^4.8.5",
    "glob": "^4.0.5"
  },
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "concatenate",
    "vendor",
    "performance",
    "scripts"
  ],
  "ember-addon": {
    "before": "broccoli-asset-rev",
    "configPath": "tests/dummy/config"
  }
}