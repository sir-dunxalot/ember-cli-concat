{
  "name": "ember-cli-concat",
  "version": "1.3.0",
  "description": "An Ember addon that enables you to concatinate Ember CLI's app and vendor files into a single JS file and a single CSS file",
  "keywords": [
    "concatenate",
    "ember",
    "ember-addon",
    "ember-cli",
    "performance",
    "scripts",
    "vendor"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sir-dunxalot/ember-cli-concat.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha node-tests/**/*-test.js"
  },
  "dependencies": {
    "broccoli-concat": "^3.7.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1"
  },
  "devDependencies": {
    "broccoli": "^0.16.9",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "ember-cli": "~3.5.0",
    "ember-cli-babel": "^5.2.8",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.12",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.0",
    "loader.js": "^4.7.0",
    "mocha": "^3.0.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "before": "broccoli-asset-rev",
    "configPath": "tests/dummy/config",
    "after": [
      "ember-component-css",
      "ember-sprite"
    ]
  }
}
